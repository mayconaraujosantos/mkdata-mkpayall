{
  "extends": "./node_modules/gts/",
  "rules": {
    "node/no-unpublished-import": [
      "error",
      {
        "allowModules": ["@jest/types"]
      }
    ],
    // @typescript-eslint
    "@typescript-eslint/no-namespace": "off",
    // import
    "import/no-duplicates": "off",
    // prettier/prettier
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    },
    "node": {
      "allowModules": [
      ]
    }
  }
}
